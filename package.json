{
  "name": "react-decinc",
  "version": "0.10.2",
  "description": "React decrement/increment input component",
  "license": "MIT",
  "repository": "VovanR/react-decinc",
  "author": {
    "name": "Vladimir Rodkin",
    "email": "mail@vovanr.com",
    "url": "https://github.com/VovanR"
  },
  "scripts": {
    "prepush": "npm test",
    "test": "xo && ava --verbose",
    "start": "webpack --watch",
    "release-patch": "mversion patch",
    "release-minor": "mversion minor",
    "release-major": "mversion major",
    "build": "webpack",
    "deploy": "gh-pages-deploy"
  },
  "files": [
    "index.jsx",
    "style.styl"
  ],
  "main": "index.jsx",
  "style": "style.styl",
  "keywords": [
    "react",
    "component",
    "react-component",
    "decrement",
    "increment",
    "input",
    "number",
    "spinner"
  ],
  "dependencies": {},
  "devDependencies": {
    "ava": "0.16.0",
    "babel-loader": "6.2.7",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-register": "6.18.0",
    "conventional-changelog-cli": "1.2.0",
    "css-loader": "0.25.0",
    "cz-conventional-changelog": "1.2.0",
    "eslint": "3.9.1",
    "eslint-config-xo": "0.17.0",
    "eslint-config-xo-react": "0.10.0",
    "eslint-plugin-react": "6.5.0",
    "husky": "0.11.9",
    "jsdom": "9.8.3",
    "mversion": "1.10.1",
    "react": "15.3.2",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "15.3.2",
    "style-loader": "0.13.1",
    "stylus": "0.54.5",
    "stylus-loader": "2.3.1",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2",
    "xo": "0.17.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "xo": {
    "extends": "xo-react"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gh-pages-deploy": {
    "staticpath": "example",
    "prep": [
      "build"
    ],
    "noprompt": false
  }
}
